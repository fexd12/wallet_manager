{
    "env": {
        "browser": true,
        "es6": true,
        "node":true,
        "mocha": true

    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly",
        "it": "writable",
        "process": "readonly"
    },
    "parserOptions": {
        "ecmaVersion":2018,
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {

    },
    "parser": "babel-eslint",
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}